{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-App",
            "request": "launch",
            "mainClass": "planto.Application",
            "projectName": "ams.kdb.backend",
            "args": [
                "--spring.profiles.active=local",
                "--spring.config.location=classpath:/config/properties/spring-config.properties,classpath:/config/properties/config.properties",
                "--spring.profiles.active=dev20",
                "--spring.config.location=classpath:/config/properties/spring-config.properties,classpath:/config/properties/config.properties"
            ],
            "vmArgs": [
                "-Dspring.profiles.active=local",
                "-Dspring.profiles.active=dev20",
            ]
        }
    ]
} 